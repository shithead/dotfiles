! '#' is for makros
URxvt*termName: rxvt-unicode-256color
URxvt*fading: 20
URxvt*font: xft:Fixed:Regular:size=14
!URxvt*font: xft:Monospace:regular:size=12
URxvt.depth: 32
URxvt.thickness: 6.5
URxvt*transparent: on
URxvt*shading: 45
!Urxvt.tabbed.tabbar-fg: 2
!Urxvt.tabbed.tabbar-bg: 0
!Urxvt.tabbed.tab-fg: 10
!Urxvt.tabbed.tab-bg: 6


URxvt.foreground: grey90
URxvt.background: gray50
URxvt.cursorBlink: true
URxvt.cursorColor: gray70
URxvt.cursorColor2: #384318
URxvt.scrollBar: on
URxvt.saveLines: 1000

URxvt.perl-ext-common:default,tabbedex,matcher,font-size
!URxvt.perl-ext-common:default,tabbed,matcher
URxvt.tabbed.autohide: true
URxvt.urlLauncher:  /usr/local/bin/firefox
URxvt.matcher.button: 3
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Up: perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

! xclock ---------------------------------------------------------------------

xclock*update:            1
xclock*analog:            false
xclock*Foreground:        white
xclock*background:        black
